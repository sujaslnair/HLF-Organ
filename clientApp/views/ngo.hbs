<body >
    <h2>GAO Dashboard</h2>

 <div class="form-modal">

    <div id="man-form">
       <h2>Assign/Match</h2>
        <form id="manform" method="POST">
            <table>
                    
            <input id="QuerypId" type="text" placeholder="Enter patient Id"/>
            <input id="QuerydId" type="text" placeholder="Enter donor Id"/>
                       
            </table>
            <button type="button" onclick="matchorg(event)" class="btn login">Match/Assign</button>
             
        </form>

    </div>
</div>      
<br>
<br>
    <div >
       <div id="first">
         <h2>Donor List</h2>
        <table id="donorlist" class="table table-striped table-hover table-bordered">
        <thead>
          <tr>
            <th scope="col">Donor Id</th>
            <th scope="col">Organ</th>
            <th scope="col">Blood group</th>
          </tr>
        </thead>
      
      </table>
       </div>

       
        <div id="second">
           <h2>     Recipient List</h2>
        <table id="patientlist" class="table table-striped table-hover table-bordered">
        <thead>
          <tr>
            <th scope="col">Patient Id</th>
            <th scope="col">Organ</th>
            <th scope="col">Blood</th>
            <th scope="col">Hospital</th>
             <th scope="col">Priority</th>
              <th scope="col">Date</th>
              <th scope="col">Donor Id</th>
          </tr>
        </thead>      
        </table>
      </div>
      </div>
     
